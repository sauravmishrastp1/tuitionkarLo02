<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".RegisterActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:id="@+id/layoutOption"
        android:layout_height="wrap_content">
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:id="@+id/cvLogin"
            android:layout_weight="1"
            android:layout_margin="10dp"

            android:layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:text="Login"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"/>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_margin="10dp"
            app:cardBackgroundColor="@color/purple_200"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:text="Register"
                android:textColor="@color/white"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_margin="10dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"/>

        </androidx.cardview.widget.CardView>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/backgnd">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <!--<ImageView
                    android:layout_width="300dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/transparent_tkl"/>-->


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:gravity="center"
                    android:text="You are just one step away from your desired job"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/action_header"
                    android:textStyle="bold"
                    android:typeface="sans" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginRight="20dp"
                    android:text="Kindly Fill The Form." />
                <RelativeLayout
                    android:layout_width="96dp"
                    android:layout_height="96dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="16dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/profileImage_iV"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:src="@drawable/defaultpic"
                        app:civ_border_color="@color/light_grey_blue"
                        app:civ_border_width="4dp" />

                    <androidx.cardview.widget.CardView
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        app:cardBackgroundColor="@color/backgnd"
                        app:cardCornerRadius="16dp"
                        app:cardElevation="0dp">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="4dp"
                            android:src="@color/bg_gray" />

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="18dp"
                            android:layout_height="18dp"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_camera_alt_black_24dp" />

                    </androidx.cardview.widget.CardView>


                </RelativeLayout>

                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_margin="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/fullName"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Owner Name"
                        android:inputType="textEmailAddress"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>



                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_margin="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/emailRegister"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Email Address"
                        android:inputType="textEmailAddress"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/phoneNumber"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Phone Number"
                        android:inputType="phone"
                        android:maxLength="10"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/whatsappNo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Whatsapp No."
                        android:inputType="phone"
                        android:maxLength="10"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:id="@+id/dobLayout"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/passwordRegister"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Date of Birth"
                        android:focusable="false"
                        android:clickable="false"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/industryType"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Industry"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/jobTitle"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Job Role ( Designation)"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                        android:id="@+id/totalExperience"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Total Experience"
                        android:paddingStart="10dp"
                        android:inputType="number"
                        android:drawableEnd="@drawable/baseline_arrow_drop_down_24"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/highestQualification"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Highest Qualification"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                        android:id="@+id/currentSalaryMonth"
                        android:layout_width="match_parent"
                        android:padding="10dp"
                        android:drawableEnd="@drawable/baseline_arrow_drop_down_24"
                        android:layout_height="match_parent"
                        android:hint=" Current Salary/Month"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                        android:id="@+id/Country"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Country"
                        android:drawableEnd="@drawable/baseline_arrow_drop_down_24"
                        android:paddingStart="10dp"
                        android:inputType="textEmailAddress"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                        android:id="@+id/state"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="State"
                        android:drawableEnd="@drawable/baseline_arrow_drop_down_24"
                        android:paddingStart="10dp"
                        android:inputType="textEmailAddress"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                        android:id="@+id/city"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="City"
                        android:drawableEnd="@drawable/baseline_arrow_drop_down_24"
                        android:paddingStart="10dp"
                        android:inputType="textEmailAddress"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/cityOther"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Other City"
                        android:paddingStart="10dp"
                        android:inputType="textEmailAddress"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                    android:layout_width="match_parent"
                    android:layout_height="56dp"
                    android:layout_margin="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginRight="16dp"
                    android:paddingStart="6dp"
                    android:theme="@style/TextLabel">

                    <EditText
                        android:id="@+id/completeAddress"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:hint="Complete Address"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/main_sub_header" />

                </com.google.android.material.textfield.TextInputLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_margin="16dp"
                    android:layout_marginRight="16dp"
                    android:orientation="horizontal"
                    android:weightSum="2">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:layout_weight="1"
                        android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            app:cardCornerRadius="10dp"
                            android:layout_margin="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_height="100dp">
                            <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="match_parent"
                                android:id="@+id/imgaadhar"
                                android:src="@drawable/baseline_cloud_upload_24"
                                android:layout_height="match_parent"/>

                        </androidx.cardview.widget.CardView>
                        <TextView
                            android:id="@+id/uploadAadhar_jobForm"
                            android:layout_width="match_parent"
                            android:layout_height="48dp"
                            android:layout_marginRight="2dp"
                            android:background="@color/colorAccent"
                            android:gravity="center"
                            android:padding="6dp"
                            android:text="Update Aadhar Card*"
                            android:textAllCaps="true"
                            android:textColor="@color/light_grey_blue"
                            android:textSize="@dimen/item_text_size"
                            android:textStyle="bold" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        android:layout_weight="1"
                        android:layout_height="wrap_content">
                        <androidx.cardview.widget.CardView
                            android:layout_width="match_parent"
                            app:cardCornerRadius="10dp"
                            android:layout_margin="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_height="100dp">
                            <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="match_parent"
                                android:id="@+id/imgresume"
                                android:src="@drawable/baseline_cloud_upload_24"
                                android:layout_height="match_parent"/>

                        </androidx.cardview.widget.CardView>
                        <TextView
                            android:id="@+id/uploadMark_jobForm"
                            android:layout_width="match_parent"
                            android:layout_height="48dp"
                            android:layout_marginLeft="2dp"
                            android:background="@color/colorAccent"
                            android:gravity="center"
                            android:padding="6dp"
                            android:text="Update Resume"
                            android:textAllCaps="true"
                            android:textColor="@color/light_grey_blue"
                            android:textSize="@dimen/item_text_size"
                            android:textStyle="bold" />
                    </LinearLayout>



                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_gravity="center_horizontal"
                    android:orientation="horizontal">


                    <TextView
                        android:id="@+id/fees"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Registration Fees"
                        android:layout_margin="16dp"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimary"
                        android:layout_weight="1"/>

                    <TextView
                        android:id="@+id/amountfees"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Rs 500/-"
                        android:textSize="16sp"
                        android:layout_weight="1"
                        android:layout_margin="16dp"
                        android:textColor="#000"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginBottom="16dp"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/checkBox_jobForm"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:buttonTint="@color/colorPrimary"
                        android:text="I've read &amp; agreed to" />

                    <LinearLayout
                        android:id="@+id/terms"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/termCondnText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="?attr/selectableItemBackground"
                            android:text=" Terms &amp; Conditions"
                            android:textColor="@color/colorAccent"
                            tools:ignore="HardcodedText" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="4dp"
                            android:background="@color/colorAccent" />


                    </LinearLayout>


                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="96dp" />

            </LinearLayout>

        </ScrollView>


        <ProgressBar
            android:id="@+id/progressBar"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_centerInParent="true"
            android:visibility="gone" />

        <TextView
            android:id="@+id/signUp"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:background="@color/colorPrimary"
            android:gravity="center"
            android:text="REGISTER"
            android:textColor="#ffffff"
            android:textSize="@dimen/main_header"
            android:textStyle="bold" />
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:id="@+id/layoutLocation"
            android:layout_margin="10dp"
            android:layout_centerInParent="true"
            app:cardBackgroundColor="#AEADAC"
            android:visibility="gone"
            android:layout_height="match_parent"
            app:cardCornerRadius="10dp"
            android:elevation="6dp">
            <LinearLayout
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_margin="5dp"
                android:orientation="vertical"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_marginBottom="20dp"
                    android:background="@color/purple_200"
                    android:layout_height="wrap_content">
                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="30dp"
                        android:id="@+id/cross"
                        android:layout_gravity="end"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="10dp"
                        android:tint="@color/white"
                        android:src="@drawable/baseline_clear_24"
                        android:layout_height="30dp"/>
                    <androidx.appcompat.widget.AppCompatTextView
                        android:layout_width="wrap_content"
                        android:id="@+id/tvLocation"
                        android:text="----Select Country---"
                        android:textStyle="bold"
                        android:textSize="14sp"
                        android:layout_marginBottom="10dp"
                        android:textColor="@color/white"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"/>

                    <com.google.android.material.textfield.TextInputLayout
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:id="@+id/layoutCountry"
                        android:layout_height="56dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="16dp"
                        android:background="@color/white"
                        android:layout_marginRight="16dp"
                        android:padding="1dp"
                        android:layout_marginBottom="10dp"
                        android:theme="@style/TextLabel">

                        <EditText
                            android:id="@+id/searchKeyWordCountry"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Search"
                            android:layout_marginEnd="5dp"
                            android:layout_marginBottom="5dp"
                            android:textColor="@color/dark_grey"
                            android:textSize="@dimen/main_sub_header" />

                    </com.google.android.material.textfield.TextInputLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:id="@+id/layoutState"
                        android:layout_height="56dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="16dp"
                        android:background="@color/white"
                        android:layout_marginRight="16dp"
                        android:padding="1dp"
                        android:layout_marginBottom="10dp"
                        android:theme="@style/TextLabel">

                        <EditText
                            android:id="@+id/searchKeyWordState"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Search"
                            android:textColor="@color/dark_grey"
                            android:textSize="@dimen/main_sub_header" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                        android:layout_width="match_parent"
                        android:id="@+id/layoutCity"
                        android:layout_height="56dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="16dp"
                        android:background="@color/white"
                        android:padding="1dp"
                        android:layout_marginRight="16dp"
                        android:layout_marginBottom="10dp"
                        android:theme="@style/TextLabel">

                        <EditText
                            android:id="@+id/searchKeyWordCity"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Search"
                            android:textColor="@color/dark_grey"
                            android:textSize="@dimen/main_sub_header" />

                    </com.google.android.material.textfield.TextInputLayout>

                </LinearLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="match_parent"
                    android:id="@+id/rvCountry1"
                    android:layout_margin="5dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    android:orientation="vertical"
                    android:layout_height="wrap_content"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="match_parent"
                    android:id="@+id/rvState1"
                    android:layout_margin="5dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    android:orientation="vertical"
                    android:layout_height="wrap_content"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="match_parent"
                    android:id="@+id/rvCity1"
                    android:layout_margin="5dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    android:orientation="vertical"
                    android:layout_height="wrap_content"/>

            </LinearLayout>


        </androidx.cardview.widget.CardView>

    </RelativeLayout>



</LinearLayout>